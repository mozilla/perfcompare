{
  "name": "perfcompare",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "@mui/system": "^5.5.1",
    "@reduxjs/toolkit": "^1.7.2",
    "@types/node": "^12.20.48",
    "@types/react": "^16.14.25",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.22",
    "notistack": "^2.0.5",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "sass": "^1.49.7",
    "serve": "^13.0.2"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-dev": "echo 'Launching local development server.' && react-scripts start",
    "start": "echo 'Launching deployment server for production builds. If you meant to run the local development server, please use `npm run start-dev` instead.' && serve -s build",
    "build": "react-scripts build",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html,md,json,yaml,yml}\" --config ./.prettierrc.js",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,html,md,json,yaml,yml}\" --config ./.prettierrc.js",
    "test": "jest --testPathIgnorePatterns=accessibility",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@swc/core": "^1.2.196",
    "@swc/jest": "^0.2.21",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-jest": "^27.5.1",
    "core-js": "^3.21.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "prettier": "^2.6.0",
    "react-scripts": "5.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/tests"
    ],
    "globalSetup": "./src/tests/utils/globalSetup.ts",
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./src/tests/utils/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "transform": {
              "react": {
                "runtime": "automatic"
              }
            }
          }
        }
      ],
      "^.+\\.svg$": "<rootDir>/src/tests/utils/fileTransformer.js"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
