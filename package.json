{
  "name": "perfcompare",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/system": "^5.8.7",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "notistack": "^2.0.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "sass": "^1.53.0",
    "serve": "^13.0.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-dev": "echo 'Launching local development server.' && react-scripts start",
    "start": "echo 'Launching deployment server for production builds. If you meant to run the local development server, please use `npm run start-dev` instead.' && serve -s build",
    "build": "react-scripts build",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html,md,json,yaml,yml}\" --config ./.prettierrc.js",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,html,md,json,yaml,yml}\" --config ./.prettierrc.js",
    "test": "jest --testPathIgnorePatterns=accessibility",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@swc/core": "^1.2.212",
    "@swc/jest": "^0.2.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.4",
    "@types/jest-axe": "^3.5.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-jest": "^28.1.2",
    "core-js": "^3.23.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-axe": "^6.0.0",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "^2.7.1",
    "react-scripts": "5.0.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "globalSetup": "<rootDir>/src/__tests__/utils/globalSetup.ts",
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/utils/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "utils"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "transform": {
              "react": {
                "runtime": "automatic"
              }
            }
          }
        }
      ],
      "^.+\\.svg$": "<rootDir>/src/__tests__/utils/fileTransformer.js"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
